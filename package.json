{
  "name": "forcemanager",
  "version": "1.0.0",
  "description": "Web testing for forcemanager",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "cypress": "^3.4.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-cypress": "^2.6.1",
    "flow-bin": "^0.81.0",
